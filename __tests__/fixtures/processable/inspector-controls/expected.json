{
  "name": "custom/input",
  "title": "Input",
  "textdomain": "input",
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "version": "0.1.0",
  "category": "theme",
  "example": {},
  "attributes": {
    "align": { "type": "string", "default": "full" },
    "checkboxAttribute": { "type": "boolean" },
    "radioAttribute": { "type": "string", "default": "yes" },
    "textAttribute": { "type": "string" },
    "toggleAttribute": { "type": "boolean" },
    "selectAttribute": { "type": "string", "default": "a" }
  },
  "supports": { "html": false, "align": ["full"] },
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}
